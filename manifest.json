{
  "manifest_version": 2,
  "name": "adSkipper",
  "version": "1.0",

  "description": "skips skipable youtube ads automatically",

  "icons": {
    "48": "icons/icon_48.png",
    "96": "icons/icon_96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["adSkipper.js"]
    }
  ]
}
